<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tidsrejseagenturet.controllers.TimeMachineController">
   <children>
      <ListView fx:id="timeMachineList" layoutX="206.0" layoutY="55.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="268.0" layoutY="31.0" text="Timemachines" />
      <TextField fx:id="nameTimeMachine" layoutX="225.0" layoutY="274.0" promptText="Name" />
      <TextField fx:id="seatsTimeMachine" layoutX="225.0" layoutY="313.0" promptText="how many seats" />
      <Button layoutX="110.0" layoutY="287.0" mnemonicParsing="false" onAction="#changeTimeMachine" text="change" />
      <Button layoutX="225.0" layoutY="350.0" mnemonicParsing="false" onAction="#addTimeMachine" text="add" />
      <Button layoutX="406.0" layoutY="287.0" mnemonicParsing="false" onAction="#confirmTimeMachine" text="confirm" />
      <Button layoutX="326.0" layoutY="350.0" mnemonicParsing="false" onAction="#removeTimeMachine" text="delete" />
   </children>
</AnchorPane>
