<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tidsrejseagenturet.controllers.CustomerViewController">
   <children>
      <TableView fx:id="customerTable" layoutX="109.0" layoutY="26.0" prefHeight="200.0" prefWidth="382.0">
        <columns>
          <TableColumn fx:id="customerIdColumn" prefWidth="75.0" text="id" />
          <TableColumn fx:id="customerNameColumn" prefWidth="135.199951171875" text="name" />
            <TableColumn fx:id="customerEmailColumn" prefWidth="172.00006103515625" text="email" />
        </columns>
      </TableView>
      <TextField fx:id="nameTextField" layoutX="225.0" layoutY="233.0" promptText="Name" />
      <Button fx:id="addButton" layoutX="220.0" layoutY="310.0" mnemonicParsing="false" onAction="#addCustomer" text="Add" />
      <Button fx:id="removeButton" layoutX="342.0" layoutY="310.0" mnemonicParsing="false" onAction="#removeCustomer" text="Remove" />
      <TextField fx:id="emailTextField" layoutX="225.0" layoutY="269.0" promptText="Email" />
      <Button fx:id="changeButton" layoutX="149.0" layoutY="256.0" mnemonicParsing="false" onAction="#changeCustomer" text="change" />
      <Button fx:id="confirmButton" layoutX="409.0" layoutY="256.0" mnemonicParsing="false" onAction="#confirmCustomer" text="confirm" />
   </children>
</AnchorPane>
