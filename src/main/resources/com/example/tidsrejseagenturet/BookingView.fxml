<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="419.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tidsrejseagenturet.controllers.BookingViewController">
   <children>
      <Label layoutX="236.0" layoutY="50.0" text="See and book your timetravel" />
      <ListView fx:id="bookingListView" fixedCellSize="0.0" layoutX="200.0" layoutY="100.0" orientation="HORIZONTAL" scaleShape="false" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <ComboBox fx:id="customersComboBox" prefWidth="150.0" promptText="CustomerID">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="timeMachineComboBox" prefWidth="150.0" promptText="TimeMachineID">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="timePeriodComboBox" prefWidth="150.0" promptText="TimePeriodeID">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="guidesComboBox" prefWidth="150.0" promptText="GuideID">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ComboBox>
            <Button fx:id="bookingButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#bookBooking" text="Book Here" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
